{
  "name": "vite-electron-builder",
  "private": true,
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "buildEnvTypes": "node scripts/buildEnvTypes.js",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir --config.asar=false",
    "pretest": "cross-env MODE=test npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "pretypecheck": "npm run buildEnvTypes",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer"
  },
  "browserslist": [
    "Chrome 91"
  ],
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.182",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.18.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^8.6.0",
    "git-tag-version": "^1.3.1",
    "lint-staged": "^11.1.2",
    "naive-ui": "^2.28.1",
    "prettier": "^2.2.1",
    "spectron": "^15.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.34.9",
    "windicss": "^3.5.1"
  },
  "dependencies": {
    "electron-store": "^8.0.1",
    "electron-updater": "^4.4.5",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gitart-vue-dialog": "^1.1.0",
    "lodash-es": "^4.17.21",
    "lowdb": "^3.0.0",
    "nanoid": "^3.3.3",
    "p-queue": "^7.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.13",
    "pretty-browser-log": "^1.0.8",
    "splitpanes": "^3.1.1",
    "v-viewer": "^3.0.10",
    "vue": "^3.2.33",
    "vue-i18n": "9",
    "vue-router": "4.0.11",
    "vue3-virtual-list": "^1.0.2",
    "vuedraggable": "^4.1.0"
  }
}
